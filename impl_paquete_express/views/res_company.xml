<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit" model="ir.ui.view">
        <field name="name">res.company.form.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[1]" position="after">
                <page string="Paquete express">
                    <group>
                        <group>
                            <field name="x_px_uri"/>                            
                        </group>
                        <group>
                            <field name="x_px_uri_ticket"/>
                        </group>
                        <group>
                            <field name="x_px_quotation_user"/>
                        </group>
                        <group>
                            <field name="x_px_quotation_password"/>
                        </group>
                        <group>
                            <field name="x_px_quotation_type"/>
                        </group>
                        <group>
                            <field name="x_px_quotation_token"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
