<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_px_shipment_tracking_form" model="ir.ui.view">
        <field name="name">px.shipment.tracking.form</field>
        <field name="model">px.shipment.tracking</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" readonly="1"/>
                    </group>
                    <field name="details" readonly="1" nolabel="1">
                        <tree editable="bottom">
                            <field name="date" readonly="1"/>
                            <field name="time" readonly="1"/>
                            <field name="status" readonly="1"/>
                        </tree>
                    </field>
                    <footer>
                        <button string="Aceptar" special="cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>